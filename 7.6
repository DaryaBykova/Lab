#include <stdio.h>
int main()
{
    float a1, b1, c1, a2, b2, c2, d, d1, d2, x, y;
    printf("Найдем корень системы линейных уравнений вида\nA1x + B1y = C1\nA2x + B2y = C1\n");
    printf("Введите коэффициент А1: ");
    scanf("%f", &a1);
    printf("Введите коэффициент В1: ");
    scanf("%f", &b1);
    printf("Введите коэффициент C1: ");
    scanf("%f", &c1);
    printf("Введите коэффициент А2: ");
    scanf("%f", &a2);
    printf("Введите коэффициент В2: ");
    scanf("%f", &b2);
    printf("Введите коэффициент C2: ");
    scanf("%f", &c2);
    d=a1*b2-a2*b1;
    d1=c1*b2-c2*b1;
    d2=a1*c2-a2*c1;
    x=d1/d;
    y=d2/d;
    printf("Решение системы уравнений - (%g, %g)", x, y);
}
