#include <stdio.h>
int main()
{
    int a, b;
    printf("Найдем наибольший общий делитель двух чисел. Введите А и В (целые и положительные): ");
    scanf("%d %d", &a, &b);
    while ((a!=0)&&(b!=0))
    {
        if (a>b)
        {
            a=a%b;
        }
        else
        {
            b=b%a;
        }
    }
    int nod=a+b;
    printf("Наибольший общий делитель A и B = %d", nod);
}
