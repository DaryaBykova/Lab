#include <stdio.h>
void poweraA3(float a, float *b)
{
   *b=a*a*a;
}
 
int main()
{
    int i;
    for(i=1;i<=5;++i){
        float a;
        printf("Найдем третью степень числа. Введите A:");
        scanf("%f", &a);
        float b;
        poweraA3(a, &b);
        printf("А^3: %g\n",b);
   }
    return 0;
}
