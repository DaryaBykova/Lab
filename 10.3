#include <stdio.h>
#include <math.h>
int main()
{
    int a;
    printf ("Проверим истинность высказывания «Данное число является четным двузначным»\n");
    printf ("Введите целое положительное число: ");
    scanf("%d", &a);
    if ((a>0)&&(a%2==0)&&(floor(a/10)>0)&&(floor(a/10)<10))
    {
        printf("Высказывание истинно.");
    }
    else 
    {
        printf("Высказывание ложно.");
    }
}
