#include <stdio.h>
int sign(float x)
{
   if (x<0){ return -1;}    
   if (x==0){ return 0;}    
   if (x>0){ return 1;}
}
int main()
{
    float a,b;
    printf("Введите A:");
    scanf("%f", &a);
    printf("Введите B:");
    scanf("%f", &b);
    printf("Sign(A)+Sign(B):%i\n",sign(a)+sign(b));
    return 0;
}
