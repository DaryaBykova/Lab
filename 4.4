#include <stdio.h>
#define _CRT_SECURE_NO_WARNINGS
int main(void)
{
    printf("Введите число a: \n");
    float a;
    scanf("%f", &a);
    printf("Введите число b: \n");
    float b;
    scanf("%f", &b);
    if ((a!=0)&& (b!=0))
    {
        float S;
        S=a*a+b*b;
        printf("Сумма квадратов введенных чисел=%f \n", S);
        float R;
        R=a*a-b*b;
        printf("Разность квадратов введенных чисел=%f \n", R);
        float P;
        P=a*a*b*b;
        printf("Произведение квадратов введенных чисел=%f \n", P);
        float C;
        C=a*a/(b*b);
        printf("Частное квадратов введенных чисел=%f \n", C);
    }
    else
    {
        printf("Ошибка");
    }
    return 0;
}
