#include <stdio.h>
#include <math.h>
#define _CRT_SECURE_NO_WARNINGS_
int main()
{
    float x, y, z, w;
    printf("Введите координаты первой точки:\n");
    scanf("%f %f", &x, &y);
    printf("Введите координаты второй точки:\n");
    scanf("%f %f", &z, &w);
    float r;
    r = sqrt((z-x)*(z-x)+(w-y)*(w-y));
    printf("Расстояние между точками=%f", r );
}
