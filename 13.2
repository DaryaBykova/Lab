#include <stdio.h>
int main()
{
    int n, i;
    float p=1, p1=1;
    printf("Введите целое число (количество сомножителей): ");
    scanf(" %d", &n);
    for (i=0; i<n;i++)
    {
        p1=p1 + 0.1;
        p=p*p1;
    }
    printf("Произведение вида 1.1 * 1.2 * 1.3 ... (N  сомножителей) = %g", p);
}
