#include <stdio.h>
int main()
{
    printf("Введите значение угла в радианах(от 0 до 2π):");
    float p;
    scanf("%f", &p);
    if ((p>=0)&&(p<=6.2831))
    {
        float x=p/3.14*180;
        printf("Значение данного угла в градусах: %g", x);
    }
    else
    {
        printf("Введено некорректное значение угла!");
    }
}
