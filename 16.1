#include <stdio.h>
int main()
{
    int n;
    printf("Введите размер массива N(N>0): ");
    scanf("%i",&n);
    int i, j = 1;
    int a[n];
    for (i = 0; i < n; i++)
    {
        a[i] = j;
        j = j + 2;
    }
    for (i=0; i<n; ++i) 
    {
        printf("a[%i] : %i\n",i,a[i]);
    }
    return 0;
}
