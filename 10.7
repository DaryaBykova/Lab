#include <stdio.h>
int main()
{
    float a, b, c;
    printf("Проверим, существует ли треугольник с заданными сторонами. Введите длины сторон: ");
    scanf("%f %f %f", &a, &b, &c);
    if ((a<(b+c))&&(b<(a+c))&&(c<(a+b)))
    {
        printf("Треугольник существует.");
    }
    else
    {
        printf("Треугольник не существует.");
    }
}
